{
  "name": "starter",
  "version": "1.0.0",
  "description": "A simple React static site development boilerplate",
  "author": "Trevor Blades",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/trevorblades/starter.git"
  },
  "scripts": {
    "start": "gulp dev",
    "dist": "gulp dist",
    "lint": "eslint src gulpfile.js"
  },
  "browserify": {
    "transform": [
      [
        "babelify",
        {
          "ignore": "node_modules",
          "plugins": [
            "transform-runtime"
          ],
          "presets": [
            "es2015",
            "react"
          ]
        }
      ]
    ]
  },
  "eslintConfig": {
    "extends": "planet/react"
  },
  "devDependencies": {
    "babel-core": "^6.26.3",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babelify": "^8.0.0",
    "browser-sync": "^2.24.2",
    "browserify": "^16.2.0",
    "del": "^3.0.0",
    "eslint": "^4.19.1",
    "eslint-config-planet": "^16.0.0",
    "eslint-plugin-react": "^7.7.0",
    "gulp": "github:gulpjs/gulp#4.0",
    "gulp-less": "^3.5.0",
    "gulp-sourcemaps": "^2.6.4",
    "gulp-uglify": "^3.0",
    "gulp-util": "^3.0.7",
    "less-plugin-autoprefix": "^1.5.1",
    "less-plugin-clean-css": "^1.5.1",
    "normalize-css": "^2.3.1",
    "proptypes": "^1.1.0",
    "react": "^16.3.2",
    "react-bootstrap": "^0.32.1",
    "react-bootstrap-typeahead": "^3.1.2",
    "react-dom": "^16.3.2",
    "vinyl-buffer": "^1.0.1",
    "vinyl-source-stream": "^2.0"
  },
  "dependencies": {
    "lodash.chunk": "^4.2.0",
    "lodash.difference": "^4.5.0",
    "lodash.flatten": "^4.4.0",
    "papaparse": "^4.1.2",
    "platform": "^1.3.1"
  }
}
